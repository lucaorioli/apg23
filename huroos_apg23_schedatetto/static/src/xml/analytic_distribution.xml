<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
   <t t-name="AnalyticDistributionPopup" t-inherit="analytic.AnalyticDistributionPopup" t-inherit-mode="extension">
       <xpath expr="//th[hasclass('numeric_column_width')]" position="before">
               <th><span>Extra</span></th>
       </xpath>
